{
    "source": {
        "include": ["src/js", "README.md"]
    },
     "templates": {
      "default": {
        "outputSourceFiles": true
      }
    },
    "opts": {
        "template" : "node_modules/docdash",
        "xtemplate" : "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "src/docs/",
        "recurse": true,
        "verbose": true,
        "xtutorials": "./tutorials",
        "theme_opts": {
            "theme": "dark",
            "favicon":"src/static/cards.png"
        }
    }
}